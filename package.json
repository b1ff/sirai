{
  "name": "sirai",
  "version": "1.0.0",
  "description": "Development Helper CLI Tool for interactive code generation with local and remote LLMs",
  "main": "dist/index.js",
  "bin": {
    "sirai": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "OPEN_AI_KEY=dummy node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "test": "mocha --exit --require ts-node/register \"test/**/*.test.ts\""
  },
  "keywords": [
    "cli",
    "development",
    "llm",
    "ai",
    "code-generation",
    "ollama",
    "openai",
    "claude"
  ],
  "author": "Eugene Moroz <sirai@emfwd.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/b1ff/sirai.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@ai-sdk/google": "^1.2.13",
    "@ai-sdk/openai": "^1.3.20",
    "ai": "^4.3.10",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.1",
    "highlight.js": "^11.9.0",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "js-yaml": "^4.1.0",
    "marked": "^15.0.7",
    "marked-terminal": "^7.3.0",
    "ollama": "^0.5.14",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^4.91.1",
    "ora": "^7.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/marked-terminal": "^6.1.1",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/sinon": "^17.0.2",
    "@types/uuid": "^9.0.7",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
